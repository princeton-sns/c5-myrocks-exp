<?xml version="1.0"?>
<parameters>
  <!-- Connection details -->
  <dbtype>mysql</dbtype>
  <driver>com.mysql.jdbc.Driver</driver>
  <DBUrl>jdbc:mysql://primary:3306/fdr</DBUrl>
  <username>root</username>
  <password></password>
  <isolation>TRANSACTION_READ_COMMITTED</isolation>
  
  <!-- This parameter has no affect on this benchmark-->
  <!-- There is no data to load -->
  <scalefactor>1</scalefactor>

  <!-- The workload -->
  <terminals>9</terminals>
  <insertspertransaction>1</insertspertransaction>
  <works>
    <work>
      <time>120</time>
      <rate>unlimited</rate>
      <weights>100,0,0,0,0,0,0</weights>
    </work>
  </works>

  <transactiontypes>
    <transactiontype>
      <name>Insert1</name>
    </transactiontype>
    <transactiontype>
      <name>Insert2</name>
    </transactiontype>
    <transactiontype>
      <name>Insert4</name>
    </transactiontype>
    <transactiontype>
      <name>Insert8</name>
    </transactiontype>
    <transactiontype>
      <name>Insert12</name>
    </transactiontype>
    <transactiontype>
      <name>Insert16</name>
    </transactiontype>
    <transactiontype>
      <name>PointRead</name>
    </transactiontype>
  </transactiontypes>
</parameters>
